public class IntroScreen {

	
	public static void main(String[] arugs)
	{
		GameFrame obj = null;
		try {
			 obj = new GameFrame();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			JOptionPane.showMessageDialog(obj,"Something went wrong");
			e.printStackTrace();
		}
	}
	
	
}
